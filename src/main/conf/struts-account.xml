   		<result name="success">/common/success.jsp</result>
    </action>
	<!-- 捐赠帐户 -->
	<action name="account" class="donationAccountAction">
   		<result name="list">/account/accountList.jsp</result>
   		<result name="add">/account/accountAdd.jsp</result>
