<struts>
	<package name="com.laungee.proj.account.web.action" extends="colavia">
	<!-- 基金大项 -->
	<action name="dxgl" class="donationDxglAction">
   		<result name="list">/account/dxglList.jsp</result>
   		<result name="add">/account/dxglAdd.jsp</result>
   		<result name="edit">/account/dxglEdit.jsp</result>
   		<result name="success">/common/success.jsp</result>
    </action>
	<!-- 基金小项 -->
	<action name="xxgl" class="donationXxglAction">
